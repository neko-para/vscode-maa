{
  "name": "vscode-maa",
  "displayName": "vscode-maa",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/neko-para/vscode-maa"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:maalog"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "semanticTokenTypes": [
      {
        "id": "date",
        "superType": "comment",
        "description": "date"
      },
      {
        "id": "source",
        "superType": "namespace",
        "description": "source"
      },
      {
        "id": "debug",
        "superType": "enum",
        "description": "debug"
      },
      {
        "id": "info",
        "superType": "enum",
        "description": "info"
      },
      {
        "id": "warning",
        "superType": "enum",
        "description": "warning"
      },
      {
        "id": "error",
        "superType": "enum",
        "description": "error"
      }
    ],
    "configurationDefaults": {
      "editor.semanticTokenColorCustomizations": {
        "rules": {
          "source": "#B58900",
          "trace": "#A1A1A1",
          "debug": "#A1A1A1",
          "info": "#00A100",
          "warning": "#A1A100",
          "error": "#FF0000"
        }
      }
    },
    "languages": [
      {
        "id": "maalog",
        "aliases": [
          "MAA日志",
          "maalog"
        ],
        "extensions": [
          ".log",
          ".maalog"
        ],
        "firstLine": " \\[\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}.\\d{3}\\]\\[[A-Z]{3}\\]\\[Px\\d+\\]\\[Tx\\d+\\]"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@vscode/vsce": "^2.21.0"
  }
}
